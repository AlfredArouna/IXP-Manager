<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Entities\PatchPanelPort" table="patch_panel_port" tableName="patch_panel_port" repository-class="Repositories\PatchPanelPort">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="position" type="integer" nullable="false"/>
    <field name="medium" type="string" length="255" nullable="false"/>
    <field name="connector" type="string" length="255" nullable="false"/>
    <field name="available_for_use" type="boolean" nullable="false"/>
    <field name="duplex" type="integer" nullable="false"/>
    <field name="colo_reference" type="string" length="255" nullable="true"/>
    <field name="ordered" type="date" nullable="true"/>
    <field name="completed" type="date" nullable="true"/>
    <field name="ceased" type="date" nullable="true"/>
    <field name="notes" type="text" nullable="true"/>
    <field name="deleted" type="boolean" nullable="false"/>
    <field name="deleted_on" type="datetime" nullable="true"/>
    <many-to-one field="PatchPanel" target-entity="Entities\PatchPanel" inversed-by="PatchPanelPorts">
      <join-columns>
        <join-column name="patch_panel_id" referenced-column-name="id" nullable="false"/>
      </join-columns>
    </many-to-one>
    <one-to-one field="Object" target-entity="Entities\PatchPanelPortObject" inversed-by="PatchPort">
      <join-columns>
        <join-column name="port_object_id" referenced-column-name="id" nullable="false" unique="true"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="PatchPanelPortObject" target-entity="Entities\PatchPanelPortObject" mapped-by="PatchPanelPort"/>
  </entity>
</doctrine-mapping>
