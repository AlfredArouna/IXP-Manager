<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Entities\PatchPanel" table="patch_panel" tableName="patch_panel" repository-class="Repositories\PatchPanel">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string" length="255" nullable="false"/>
    <field name="medium" type="string" length="255" nullable="false"/>
    <field name="connector" type="string" length="255" nullable="false"/>
    <field name="duplex_allowed" type="boolean" nullable="false"/>
    <field name="notes" type="text" nullable="true"/>
    <field name="u_position" type="integer" nullable="false"/>
    <field name="installed" type="date" nullable="false"/>
    <many-to-one field="Cabinet" target-entity="Entities\Cabinet" inversed-by="PatchPanel">
      <join-columns>
        <join-column name="cabinet_id" referenced-column-name="id" nullable="false"/>
      </join-columns>
    </many-to-one>
    <one-to-many field="PatchPanelPorts" target-entity="Entities\PatchPanelPort" mapped-by="PatchPanel"/>
  </entity>
</doctrine-mapping>
